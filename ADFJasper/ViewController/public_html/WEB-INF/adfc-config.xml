<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <url-view id="toReportServlet">
    <url>#{jasperBean.currentContext}</url>
  </url-view>
  <control-flow-rule id="__2">
    <from-activity-id>*</from-activity-id>
    <control-flow-case id="__3">
      <from-outcome>toReportServlet</from-outcome>
      <to-activity-id>toReportServlet</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
  <managed-bean id="__1">
    <managed-bean-name>jasperBean</managed-bean-name>
    <managed-bean-class>view.JasperBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
  </managed-bean>
</adfc-config>
