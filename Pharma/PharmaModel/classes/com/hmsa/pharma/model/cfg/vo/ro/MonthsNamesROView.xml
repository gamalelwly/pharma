    Name="MonthName"
    IsUpdateable="false"
    IsPersistent="false"
    PrecisionRule="true"
    Precision="9"
    Type="java.lang.String"
    ColumnType="VARCHAR2"
    AliasName="MONTH_NAME"
    Expression="MONTH_NAME"
    SQLType="VARCHAR">
    <DesignTime>
      <Attr Name="_DisplaySize" Value="9"/>
    </DesignTime>
  </ViewAttribute>
  <AttrArray Name="KeyAttributes">
    <Item Value="Id"/>
  </AttrArray>
  <ResourceBundle>
    <PropertiesBundle
      PropertiesFile="com.hmsa.pharma.model.bundles.PharmaModelBundle"/>
  </ResourceBundle>
</ViewObject>
