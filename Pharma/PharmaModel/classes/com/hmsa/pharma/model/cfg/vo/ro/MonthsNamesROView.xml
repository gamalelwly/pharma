<!DOCTYPE ViewObject SYSTEM "jbo_03_01.dtd">
<!---->
<ViewObject
  xmlns="http://xmlns.oracle.com/bc4j"
  Name="MonthsNamesROView"
  Version="12.2.1.12.24"
  InheritPersonalization="merge"
  BindingStyle="OracleName"
  CustomQuery="true"
  RowClass="com.hmsa.checkpoint.basic.classes.CheckpointViewRowImpl"
  ComponentClass="com.hmsa.checkpoint.basic.classes.CheckpointViewObjectImpl"
  DefClass="com.hmsa.checkpoint.basic.classes.CheckpointViewDefImpl"
  PageIterMode="Full">
  <DesignTime>
    <Attr Name="_isExpertMode" Value="true"/>
  </DesignTime>
  <Properties>
    <SchemaBasedProperties>
      <LABEL
        ResId="com.hmsa.pharma.model.cfg.vo.ro.MonthsNamesROView_LABEL"/>
    </SchemaBasedProperties>
  </Properties>
  <Variable
    Name="bindLocale"
    Kind="where"
    Type="java.lang.String">
    <TransientExpression
      Name="VariableScript"
      trustMode="trusted"
      CodeSourceName="MonthsNamesROView"/>
  </Variable>
  <SQLQuery><![CDATA[SELECT ID, MONTH_NAME  
  FROM MONTHS_NAMES_V  
WHERE   
 CASE WHEN UPPER (:bindLocale) = 'AR' THEN   
